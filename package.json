{
  "name": "raas-simulation",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "Educational RaaS simulation built with React, TypeScript, and Node.js - Safe & Non-Malicious",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "vite",
    "server": "node src/backend/server_sqlite.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "init-db": "node src/backend/db_init_sqlite.js",
    "seed-db": "node src/backend/seed_data_sqlite.js",
    "start": "node src/backend/server_sqlite.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "socket.io": "^4.7.5",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}